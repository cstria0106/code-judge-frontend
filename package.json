    "@types/lodash": "^4.14.198",
    "@types/pako": "^2.0.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.66.1",
    "svelte": "^4.0.5",
