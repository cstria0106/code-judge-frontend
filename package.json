    "@codemirror/lang-markdown": "^6.2.0",
    "@uiw/codemirror-theme-github": "^4.21.13",
    "buffer": "^6.0.3",
    "carbon-components-svelte": "^0.80.0",
    "carbon-icons-svelte": "^12.3.0",
    "date-picker-svelte": "^2.6.0",
    "filesize": "^10.0.12",
    "highlight.js": "^11.8.0",
    "lodash": "^4.17.21",
    "marked": "^8.0.1",
    "marked-highlight": "^2.0.5",
    "moment": "^2.29.4",
    "pako": "^2.1.0",
    "rxjs": "^7.8.1",
